{
  "functions": {
    "src/app/ads.txt/route.ts": {
      "maxDuration": 10
    }
  },
  "headers": [
    {
      "source": "/ads.txt",
      "headers": [
        {
          "key": "Content-Type",
          "value": "text/plain"
        },
        {
          "key": "Cache-Control",
          "value": "public, max-age=86400"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/ads.txt",
      "destination": "/ads.txt"
    }
  ]
}